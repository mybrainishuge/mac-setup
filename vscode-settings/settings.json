{
    "[c]": { "editor.quickSuggestions": false },
    "[cpp]": { "editor.quickSuggestions": false },
    "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
    "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
    "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "breadcrumbs.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "block",
    "editor.fontSize": 10,
    "editor.hideCursorInOverviewRuler": true,
    "editor.lineHeight": 0,
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": false,
    "editor.renderIndentGuides": true,
    "editor.renderWhitespace": "all",
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "emmet.showExpandedAbbreviation": "never",
    "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact", "graphql"],
    "explorer.openEditors.visible": 9,
    "extensions.autoUpdate": true,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.keymap": "chorded",
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "php.validate.run": "onType",
    "prettier.semi": false,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "es5",
    "telemetry.enableTelemetry": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontSize": 10,
    "terminal.integrated.rendererType": "dom",
    "timeline.excludeSources": [],
    "typescript.updateImportsOnFileMove.enabled": "always",
    "window.restoreFullscreen": true,
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "editor.findMatchHighlightBackground": "#73119A",
        "editor.lineHighlightBackground": "#000000",
        "editor.selectionBackground": "#00791B",
        "editor.selectionHighlightBackground": "#73119A",
        "editorCursor.foreground": "#4af76f",
        "statusBar.background": "#151515",
        "statusBar.border": "#555555",
        "tab.activeBackground": "#415968",
        "tab.border": "#999999"
    },
    "workbench.colorTheme": "Gruvbox Dark Hard",
    "workbench.commandPalette.preserveInput": true,
    "workbench.editor.closeOnFileDelete": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.revealIfOpen": true,
    "workbench.iconTheme": "vscode-icons",
    "workbench.sideBar.location": "left",
    "workbench.startupEditor": "none",
    "python.formatting.provider": "black",
    "explorer.confirmDragAndDrop": false,
    "cSpell.userWords": [
        "babylonjs",
        "bitly",
        "camelcase",
        "chunkhash",
        "codecov",
        "collapser",
        "commenters",
        "Coord",
        "corge",
        "csstype",
        "dataloader",
        "descendents",
        "devtool",
        "Distributees",
        "draggable",
        "Droppable",
        "eloqua",
        "embeddable",
        "eqeqeq",
        "esnext",
        "falconhood",
        "fieldsets",
        "focusable",
        "Fullscreen",
        "Funcs",
        "gmail",
        "gplus",
        "grault",
        "gridcell",
        "Gsuite",
        "Haribo",
        "Healthcheck",
        "hemi",
        "Hotlanta",
        "hoverable",
        "hueco",
        "Hueco",
        "immer",
        "Inactives",
        "Jira",
        "keypresses",
        "kona",
        "kubernetes",
        "labelledby",
