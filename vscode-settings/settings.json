// Place your settings in this file to overwrite the default settings
{
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "block",
  "editor.hideCursorInOverviewRuler": true,
  "editor.renderControlCharacters": false,
  "editor.renderIndentGuides": true,
  "editor.renderWhitespace": "all",
  "editor.tabCompletion": true,
  "editor.tabSize": 2,
  "extensions.autoUpdate": true,
  "files.trimTrailingWhitespace": true,
  "telemetry.enableTelemetry": false,
  "terminal.external.osxExec": "iTerm.app",
  "vsicons.dontShowNewVersionMessage": true,
  "vsicons.projectDetection.disableDetect": true,
  "window.restoreFullscreen": true,
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Gruvbox Dark (Medium Contrast)",
  "workbench.iconTheme": "vscode-icons",
  "workbench.sideBar.location": "left",
  "workbench.statusBar.visible": true,
  "workbench.colorCustomizations": {
    "tab.activeBackground": "#415968",
    "tab.border": "#999999",
    "statusBar.border": "#555555",
    "editorCursor.foreground": "#4af76f",
    "editor.findMatchHighlightBackground": "#73119A",
    "editor.lineHighlightBackground": "#000000",
    "editor.selectionBackground": "#00791B",
    "editor.selectionHighlightBackground": "#73119A",
    "statusBar.background": "#151515"
  },
  "workbench.editor.closeOnFileDelete": false,
  "workbench.editor.revealIfOpen": true,
  "[cpp]": {
    "editor.quickSuggestions": false
  },
  "[c]": {
    "editor.quickSuggestions": false
  },
  "workbench.commandPalette.preserveInput": true,
  "workbench.startupEditor": "none",
  "terminal.integrated.fontSize": 10,
  "emmet.showExpandedAbbreviation": "never",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.enablePreview": false,
  "php.validate.run": "onType",
  "explorer.openEditors.visible": 9,
  "files.insertFinalNewline": true,
  "gitlens.keymap": "chorded",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "editor.lineHeight": 0,
  "gitlens.historyExplorer.enabled": true,
  "editor.fontSize": 10,
  "editor.minimap.enabled": false,
  "window.zoomLevel": 0,
  "terminal.integrated.rendererType": "dom",
  "prettier.singleQuote": true,
  "prettier.eslintIntegration": true,
  "prettier.printWidth": 100,
  "prettier.trailingComma": "es5",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "breadcrumbs.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.formatOnSave": true,
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true
}
